{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "[transformar]",
    "namespace": "[transformar]"
  },
  "spec": {
    "ports": [
      {
        "port": 80,
        "targetPort": 80,
        "name": "http-port"
      },
      {
        "port": 443,
        "targetPort": 80,
        "name": "https-port"
      }
    ],
    "selector": {
      "app": "[transformar]"
    }
  }
}