{
  "apiVersion": "v1",
  "kind": "Service",
  "metadata": {
    "name": "__nomeapp__",
    "namespace": "sefazsp-linux"
  },
  "spec": {
    "ports": [
      {
        "port": 80,
        "targetPort": 80,
        "name": "http-port"
      },
      {
        "port": 443,
        "targetPort": 80,
        "name": "https-port"
      }
    ],
    "selector": {
      "app": "__nomeapp__"
    }
  }
}